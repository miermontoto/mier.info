---
layout: base.njk
css: generic
---

<div id="title-container">
    <span id="breadcrumbs">
        {% set navPages = collections.all | eleventyNavigation %}
        {% breadcrumbs navPages %}
    </span>
    <span id="title">{{ title }}</span>
    {% if subtitle %}
        <span id="subtitle">{{ subtitle }}</span>
    {% endif %}
    {% if created_at %}
        <span class="date">{{ created_at }}</span>
    {% endif %}
</div>

{{ content | safe }}

---
layout: base.njk
css: project
---

<div id="title-container">
    <span id="breadcrumbs">
        {% set navPages = collections.all | eleventyNavigation %}
        {% breadcrumbs navPages %}
    </span>
    <span id="title">{{ title }}</span>
</div>
<div id="project-buttons">
    {% for button in buttons %}
        <a class="button" href="{{ button.url }}">{{ button.img | safe }}{{ button.text }}</a>
    {% endfor %}
    {% if source %}
        <a href="{{ source }}" id="source" class="button"><img src="/assets/icons/tech/git.svg" class="icon invert" alt="source icon">source</a>
    {% endif %}
</div>
<hr>
{% if content %}
    {{ content | safe }}
{% else %}
    <br> work in progress!
{% endif %}

{% include "src/templates/snippets/footer.njk" %}

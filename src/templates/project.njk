---
layout: base.njk
css: project
---

<a href="{{ back or "/" }}" id="back">< go back</a>
{% if source %}
    <span id="sep"> | </span>
    <a href="{{ source }}" id="source">source code ></a>
{% endif %}
<h1>{{ title }}</h1>
{% for button in buttons %}
    <a class="button" href="{{ button.url }}">{{ button.img | safe }} {{ button.text }}</a>
{% endfor %}
<hr>
{% if content %}
    {{ content | safe }}
{% else %}
    <br> work in progress!
{% endif %}

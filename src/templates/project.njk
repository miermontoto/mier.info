---
layout: base.njk
css: /static/css/project.css
---

{% if project_css %}
    {% set temp = title if project_css === true else project_css %}
    {% set target = '/static/css/project/' + temp + '.css' %}
    <link rel="stylesheet" href="{{ target }}">
{% endif %}

{% if project_js %}
    {% set temp = title if project_js === true else project_js %}
    {% set target = '/static/js/project/' + temp + '.js' %}
    <script src="{{ target }}"></script>
{% endif %}

<a href="/" id="back">< go back</a>
{% if source %}
    <span id="sep"> | </span>
    <a href="{{ source }}" id="source">source code ></a>
{% endif %}
<h1>{{ title }}</h1>
<hr>
{% if content %}
    {{ content | safe }}
{% else %}
    <br> work in progress!
{% endif %}

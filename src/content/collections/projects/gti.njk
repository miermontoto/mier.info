{% addStyle 'gti' %}
{% addScript 'gti' %}

<div id="results">
	<span id="correct">0</span> / <span id="total">?</span>
	<span id="→">→</span>
	<span id="scored"><span id="score">0</span>%</span>
	<noscript>ERROR: JavaScript is NEEDED for the quiz to work!</noscript>
</div>
<span class="button" id="reset"><img src="/assets/icons/button/reset.svg" class="icon invert" alt="reset"> reset</span>
<div id="options">
	<h2>opciones.</h2>
	<input type="checkbox" id="shuffle-questions" checked>
	<label for="shuffle-questions">saltear preguntas</label>
	<br>
	<input type="checkbox" id="shuffle-answers" checked>
	<label for="shuffle-answers">saltear respuestas</label>
</div>

